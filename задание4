#include <iostream>
#include <ctime>
using namespace std;
void print_array(int array[], int size) {
	for (int i = 0; i < size; i++) {
		cout << array[i] << " ";
	}
	cout << endl;
}
int random_number() {
	return rand() % 90 + 10;
}
void random_array(int array[], int size) {
	for (int i = 0; i < size; i++) {
		array[i] = random_number();
		int tmp = array[0];
	}

}
void sdvig(int array[], int size) {
	int tmp = array[0];
	for (int i = 0; i < size - 1; i++) {
	array[i] = array[1 + i];
}
	array[size - 1] = tmp;
	size = 100;
}
int main() {
	srand(time(0));
	const int n = 30;
	int mas[n];
	random_array(mas, n);
	print_array(mas, n);
	for (int i = 0; i < 3; i++)
		sdvig(mas, n);
	print_array(mas, n);

} 
